#include <iostream>
#include <math.h>
#include <algorithm>
#include <iomanip>
#include <set>
#include <string.h>
#include <map>
#include <vector>
using namespace std;
int main(){
    int n,x,sum = 0;
    cin>>n>>x;
    int arr[n+1];
    for(int i = 1;i<=n;i++){
        cin>>arr[i];
        if(i%2==1) sum+=arr[i];
        else sum+=(arr[i]-1);
        }
        if(x>=sum) cout<<"Yes"<<endl;
        else cout<<"No"<<endl;
        return 0;
    }
